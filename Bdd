DROP TABLE IF EXISTS centrale ;
CREATE TABLE centrale (id_Centrale INT(5) AUTO_INCREMENT NOT NULL,
adr_Centrale VARCHAR(40),
ville_Centrale VARCHAR(25),
cp_Centrale INT(5),
tel_Centrale VARCHAR(10),
capaciteProd_Centrale INT(6),
responsable_matricule_responsable **NOT FOUND**(5),
logisticien_matricule_logisticien **NOT FOUND**(5),
PRIMARY KEY (id_Centrale)) ENGINE=InnoDB;

DROP TABLE IF EXISTS camion ;
CREATE TABLE camion (immat_camion VARCHAR(7) AUTO_INCREMENT NOT NULL,
hDepart_camion TIME,
hArrive_camion TIME,
hRetour_camion TIME,
nbTour_camion INT(5),
tpsNettoyage_camion TIME,
tpsRemplissage_camion TIME,
chauffeur_matricule_chauffeur **NOT FOUND**(7),
PRIMARY KEY (immat_camion)) ENGINE=InnoDB;

DROP TABLE IF EXISTS contrat ;
CREATE TABLE contrat (num_Contrat INT(5) AUTO_INCREMENT NOT NULL,
adr_Contrat VARCHAR(40),
ville_Contrat VARCHAR(10),
cp_Contrat INT(5),
dateDebut_Contrat DATE,
dateFin_Contrat DATE,
qteALivrer_Contrat DECIMAL(4),
num_client **NOT FOUND**(5),
PRIMARY KEY (num_Contrat)) ENGINE=InnoDB;

DROP TABLE IF EXISTS responsable ;
CREATE TABLE responsable (matricule_Responsable INT(6) AUTO_INCREMENT NOT NULL,
nom_Responsable VARCHAR(25),
prenom_Responsable VARCHAR(25),
centrale_id_centrale **NOT FOUND**(6),
PRIMARY KEY (matricule_Responsable)) ENGINE=InnoDB;

DROP TABLE IF EXISTS mission ;
CREATE TABLE mission (num_mission INT(5) AUTO_INCREMENT NOT NULL,
dateTransport_mission DATE,
heureTransport_mission TIME,
qteAtransporter_mission DECIMAL(4),
matricule_Logisticien **NOT FOUND**(5),
PRIMARY KEY (num_mission)) ENGINE=InnoDB;

DROP TABLE IF EXISTS chauffeur ;
CREATE TABLE chauffeur (matricule_Chauffeur INT(6) AUTO_INCREMENT NOT NULL,
nom_Chauffeur VARCHAR(25),
prenom_Chauffeur VARCHAR(25),
camion_immat_camion VARCHAR(7),
mission_num_mission INT(5),
id_Centrale **NOT FOUND**(6),
PRIMARY KEY (matricule_Chauffeur)) ENGINE=InnoDB;

DROP TABLE IF EXISTS logisticien ;
CREATE TABLE logisticien (matricule_Logisticien INT(6) AUTO_INCREMENT NOT NULL,
nom_Logisticien VARCHAR(25),
prenom_Logisticien VARCHAR(25),
PRIMARY KEY (matricule_Logisticien)) ENGINE=InnoDB;

DROP TABLE IF EXISTS client ;
CREATE TABLE client (num_client INT(5) AUTO_INCREMENT NOT NULL,
nom_client VARCHAR(100),
adr_client VARCHAR(40),
ville_client VARCHAR(25),
cp_client INT(5),
tel_client VARCHAR(10),
PRIMARY KEY (num_client)) ENGINE=InnoDB;

DROP TABLE IF EXISTS estGere ;
CREATE TABLE estGere (num_Contrat **NOT FOUND**(5) AUTO_INCREMENT NOT NULL,
matricule_Logisticien **NOT FOUND**(5) NOT NULL,
PRIMARY KEY (num_Contrat,
 matricule_Logisticien)) ENGINE=InnoDB;

ALTER TABLE centrale ADD CONSTRAINT FK_centrale_responsable_matricule_responsable FOREIGN KEY (responsable_matricule_responsable) REFERENCES responsable (matricule_Responsable);

ALTER TABLE centrale ADD CONSTRAINT FK_centrale_logisticien_matricule_logisticien FOREIGN KEY (logisticien_matricule_logisticien) REFERENCES logisticien (matricule_Logisticien);
ALTER TABLE camion ADD CONSTRAINT FK_camion_chauffeur_matricule_chauffeur FOREIGN KEY (chauffeur_matricule_chauffeur) REFERENCES chauffeur (matricule_Chauffeur);
ALTER TABLE contrat ADD CONSTRAINT FK_contrat_num_client FOREIGN KEY (num_client) REFERENCES client (num_client);
ALTER TABLE responsable ADD CONSTRAINT FK_responsable_centrale_id_centrale FOREIGN KEY (centrale_id_centrale) REFERENCES centrale (id_Centrale);
ALTER TABLE mission ADD CONSTRAINT FK_mission_matricule_Logisticien FOREIGN KEY (matricule_Logisticien) REFERENCES logisticien (matricule_Logisticien);
ALTER TABLE chauffeur ADD CONSTRAINT FK_chauffeur_camion_immat_camion FOREIGN KEY (camion_immat_camion) REFERENCES camion (immat_camion);
ALTER TABLE chauffeur ADD CONSTRAINT FK_chauffeur_mission_num_mission FOREIGN KEY (mission_num_mission) REFERENCES mission (num_mission);
ALTER TABLE chauffeur ADD CONSTRAINT FK_chauffeur_id_Centrale FOREIGN KEY (id_Centrale) REFERENCES centrale (id_Centrale);
ALTER TABLE estGere ADD CONSTRAINT FK_estGere_num_Contrat FOREIGN KEY (num_Contrat) REFERENCES contrat (num_Contrat);
ALTER TABLE estGere ADD CONSTRAINT FK_estGere_matricule_Logisticien FOREIGN KEY (matricule_Logisticien) REFERENCES logisticien (matricule_Logisticien);
